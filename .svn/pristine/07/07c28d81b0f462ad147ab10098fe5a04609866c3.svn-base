@charset "utf-8";
/* CSS Document */

#header {
    background-image:url('../_images/searchBackground.png');
    background-repeat:repeat;
    margin:0;
    font: white;
    width:100%;
    padding:0;
    display:inline-block;
}
a {
    font-size:14px;	
}
h1 {
    color:#FFFFFF;
    font-family:helvetica;
    float:left;
    display:inline-block;
    margin-top:35px;	
}
h2 {
    color:black;
    font-family: calibri;
    padding:0px;
    margin:5px 0px 10px 0px
}
h3 {
    margin: 0px 0px 10px 0px;
}
body {
    font-family:calibri;
    /*background-color:#CCC;*/
    background-color:#EDEDF1;
    padding:0;
    border:0;
    margin:0 auto;
    width:100%;
    height:100%;
}
#sapLogo {
    float:left;
    border:0px;
    width:auto;
    padding:0px;
    margin:0;
}
#searchOption {
    float:left;
    display:inline-block;
    width:500px;
}
#sort {
    margin:0px;
    float:left;
}
#days {
    margin-left:5px;
    float:left;
}
#match {
    margin-left:5px;
    float:left;	
}
.orangeBreak {
    background-image:url(../_images/sapOrange.jpg);
    background-repeat:repeat;
    height:12px;
    width:100%;
    margin:0px;
    padding:0px;
    display:block;
    float:none;
}
#homePageFiller {
    background-color:#CCC;
    margin:0px;
    display:block;
    height:100%;
    width:100%;
}
#content {
    background-color:#FFF;
    margin:10px auto 0 auto;
    padding:0 15px 25px 15px;
    display:inline-block;
    /*width:auto;*/
    width:960px;
    -webkit-box-shadow: #333 0px 0px 15px;
    -moz-box-shadow: #333 0px 0px 15px;
    box-shadow: 0 0 15px #333;
    text-align:left;
}
#internal {
    /*width:784px;*/
    width:100%;
    float:left;
}
.resultsType{}
.external-source{}
.external-name{}
.external-results{}
#external {
    /*width:176px;*/
    width:100%;
    float:left;

}
#container-page {
    margin:0 auto;
    overflow:visible;
    float:none;
    display:block;
    text-align:center;
}
.entry {
    width:870px;
    height:auto;
    margin:5px 0;
}
.entry-title {
    width:800px;
    height:auto;	
    float:left;
}
.entry-date {
    width:70px;
    height:auto;
    float:right;
}
.entry-description {
    width:auto;
    height:auto;	
    float:left;
}
.entry-url {
    width:auto;
    height:auto;
    float:left;
    font-style:oblique;
}
.entry-source {
    width:auto;
    height:auto;
    float:left;
    font-style:italic oblique;
}